FAIRY_AREA_CORE_MAKE = {
    SCA_core_make = {
		icon = onmap_recruit_operative
		available = {
			
		}
		state_target = yes
		target_trigger = {
			FROM = {
				is_owned_by = ROOT
				is_fully_controlled_by = ROOT
                not = {is_core_of = ROOT}
				is_core_of = SCA
			}
		}
		visible = {
			is_fairy_area = yes 
			has_country_flag = FIR_AREA_SCA_MAKE_CORE 
            not = {
                tag = SCA
            }
			
		}

		fire_only_once = yes
		cost = var:ROOT.FIR_AREA_SCA_MC_cost 

		days_re_enable = 0
		days_remove = var:ROOT.FIR_AREA_SCA_MC_time 

		#on_map_mode = map_only
		complete_effect = {
			
		}
		remove_effect = {
			FROM = {add_core_of = ROOT}
		}
		modifier = {
			
		}

		ai_will_do = {
			base = 1000
		}
	}
