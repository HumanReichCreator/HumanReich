SC2_infl_category = {
	SC2_simple_give_infl = {
		visible = {
			ROOT = {
				SC2_can_infl = yes
			}
		}
		available = {
			
		}
		modifier = {
		}
		cost = 30
		days_remove = 30
		days_re_enable = 3
		fire_only_once = no
		complete_effect = {
			add_to_variable = { ROOT.SC2_infl_value = 5 }
			cal_FPA_infl = yes
		}
		remove_effect = {
			
		}
		ai_will_do = {
			factor = 1
			modifier = {
				add = 30
				SC2_need_pay_attention = yes
			}
		}
	}
	SC2_debug_give_infl = {
		visible = {
			ROOT = {
				SC2_can_infl = yes
			}
			is_debug = yes
		}
		available = {
			
		}
		modifier = {
		}
		cost = 30
		days_remove = 3
		days_re_enable = 3
		fire_only_once = no
		complete_effect = {
			SC2 = { set_country_flag = FA_in_FPA_flg }
			CIR = { set_country_flag = FA_in_FPA_flg }
			LIL = { set_country_flag = FA_in_FPA_flg }
			add_to_variable = { ROOT.SC2_infl_value = 50 }
			set_variable = { CIR.SC2_infl_value = 25 }
			set_variable = { LIL.SC2_infl_value = 25 }
			cal_FPA_infl = yes

		}
		remove_effect = {
			
		}
		ai_will_do = {
			factor = 0
			
		}
	}
	CIR_debug_guli = {
