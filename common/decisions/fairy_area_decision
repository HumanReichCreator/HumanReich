	CIR_guli_task_one_misson = {
		allowed = { always = no  }
		available = {
			has_completed_focus = SGS_integrate_SCA_focus	
			has_completed_focus = SGS_integrate_CIR_focus
			has_completed_focus = SGS_integrate_LIL_focus		# 任务目标
			has_country_flag = sgs_all_integrated_peacelly
		}
		activation = {
			always = no
		}
		is_good = yes
		selectable_mission = yes
		days_mission_timeout = var:CIR.CIR_TASK_time
		timeout_effect = {
			country_event = { id = sgs.4031 }
			#不服
		}
		complete_effect = {
